        </jdbcConnection>

        <javaModelGenerator targetPackage="com.code.data.beans"
                            targetProject="/Users/chancelee/Applications/newsmanager/data-service/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper"
                         targetProject="/Users/chancelee/Applications/newsmanager/data-service/src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.code.data.dao"
        targetProject="/Users/chancelee/Applications/newsmanager/data-service/src/main/java">
        <property name="enableSubPackages" value="true"/>
